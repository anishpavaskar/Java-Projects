package savings;


import java.util.Arrays;

public class SavingsAccount{

	public static void main(String[]args)
		
	{
    	int[] array= new int[11];
    	int[] b= new int[34];
		Arrays.fill(array,1);
		Arrays.fill(b, 0);
		System.arraycopy(array, 0, b, 0, array.length);
		for(int i =1; i < b.length; i++)
		{
			System.out.printf("%d ", b[i]);
		}
		
		
	}
    			   
    			   
    	
}
