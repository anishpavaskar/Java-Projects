package savings;

import java.util.Arrays;

public class SavingsAccount
{
    	public static void main(String[] args) {
    	 int[] g = new int[5];
    	 Arrays.fill(g, 8);
    	 for(int i=0; i < g.length; i++)
    	 {
    		 System.out.printf("g[%d}=%d%n",i,g[i]);
    	 }
    			   
    			   
    	}
        

    }


