package exercise;


import java.util.Scanner;



public class exercises {

	public static void main(String[] args) 
	{
		 // init vars
		int x;
		int y;
		// asks user to 2 integers 
		System.out.println("Enter two integers");
		Scanner in= new Scanner (System.in);
		
		x=in.nextInt();
		y=in.nextInt();
	
		in.close();
		
		// finds out if one number is larger than the other, or if they are equal. Then prints out a result 
		  if ( x > y )
		  {
			  System.out.printf( " %d  is a larger number", x) ;
		  }
		  
		  if ( y > x )
		  {
			  System.out.printf( " %d  is a larger number", y) ;
		  }
		  if (y ==x )
		  {
			  System.out.printf( " These numbers are equal" ) ;
		  }
	}	
	
}	

